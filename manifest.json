{
  "version": "0.0.1",
  "manifest_version": 3,
  "name": "Jira Deactivation Banner Remover",
  "author": {
    "url": "https://www.tcmhack.in",
    "name": "TIKAM CHAND MEGHWANSHI"
  },
  "description": "Removes \"Jira Software will be deactivated\" banner",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "all_frames": false,
      "run_at": "document_end",
      "js": ["content_script.js"],
      "matches": ["https://*.atlassian.net/*"]
    }
  ]
}
